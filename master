	bindLoad.Position = UDim2.new(0.752, 0, 0.115, 0)
	bindLoad.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	bindLoad.BorderSizePixel = 2
	bindLoad.BorderColor3 = Color3.fromRGB(0, 0, 0)
